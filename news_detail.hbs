    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{{ this.page.title }}</title>
        <meta name="description" content="{{ this.page.desc }}">
       

        <link rel="stylesheet" href="{{language.dir}}css/normalize.min.css">
        <link rel="stylesheet" href="{{language.dir}}css/main.css">
        <link rel="stylesheet" href="{{language.dir}}css/animate.css">
        <link rel="stylesheet" href="{{language.dir}}fonts/vegur/stylesheet.css">
		<link rel="stylesheet" href="{{language.dir}}css/bootstrap-theme.min.css">
		<link rel="stylesheet" href="{{language.dir}}css/bootstrap-theme.css">
		<link rel="stylesheet" href="{{language.dir}}css/bootstrap.min.css">
		<link rel="stylesheet" href="{{language.dir}}css/bootstrap.css">
        <!-- JS includes -->
        <script type="text/javascript">
            document.write('\x3Cscript src="' + (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//webfonts.creativecloud.com/open-sans:n3,n4,i4:all.js" type="text/javascript">\x3C/script>');
        </script>


        <script src="{{language.dir}}js/vendor/modernizr-2.6.2.min.js"></script>
		<script src="{{language.dir}}js/vendor/bootstrap.min.js"></script>
		<script src="{{language.dir}}js/vendor/jquery.dropdown.min.js"></script>
		<script src="{{language.dir}}js/vendor/jquery.parallax-1.1.3.js"></script>
		<script src="{{language.dir}}js/vendor/jquery-1.11.1.min.js"></script>
    </head>
    <body>
    {{>dlm-header}}
     <section id="news_notices" data-title="{{ page.title }}">
       <div class="clearfix d_dividerbar">&nbsp;</div>
       <div class="clearfix d_section d_notices">
            <div class="clearfix  d_top ">
            <div class="container">
                <div class="d_titlebar" >
                  <h1 >Latest News</h1>
                </div>
            </div>    
            </div>
            <div class="clearfix d_dividerbar cyan" >  </div>
            <div class="clearfix" >
               <div class="container" >
               <div class="d_copy" >
               <h2>{{ this.page.title }}</h2>
               <p class="d_date">{{formatDate this.page.posted "%B %d, %Y"}}</p>
               {{>body}}
              </div>
               </div>

            </div>
           
       </div>
       </section>

    {{>dlm-footer}}
  
        <!--
        // use local Jquery version during development 
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
        -->
        <script>
        window.jQuery || document.write('<script src="{{language.dir}}js/vendor/jquery-1.11.1.min.js"><\/script>')
        </script>

        
        <script src="{{language.dir}}js/slideshow.js" type="text/javascript"></script>
         <script src="{{language.dir}}js/vendor/jquery.parallax-1.1.3.js"></script>
         <script type="text/javascript" src="{{language.dir}}js/vendor/jquery.dropdown.min.js"></script>
         <script src="{{language.dir}}js/plugins.js"></script>
        <script src="{{language.dir}}js/main.js"></script>
        {{#block "script"}}
        {{/block}}
       
        <!--  sales fusion -->
        <script type="text/javascript" language="JavaScript" src="http://go.delmarcargo.com/js/frs-app-96654.js"></script>
        <script type="text/javascript">frt(96654);</script>

        <!-- Google Analytics -->
        <script>
            var _gaq=[['_setAccount','UA-5202853-1'],['_trackPageview']];
            (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
            g.src='//www.google-analytics.com/ga.js';
            s.parentNode.insertBefore(g,s)}(document,'script'));
        </script>
    </body>
</html>
